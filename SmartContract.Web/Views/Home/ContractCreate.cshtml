@{
    ViewData["Title"] = "สร้างข้อมูลนิติกรรมใหม่";
}

<link href="~/css/home/progressbar.css" rel="stylesheet" />
<link href="~/css/formcontract/formcontract.css" rel="stylesheet" asp-append-version="true" />


<section>
    <div class="">
        <div class="col-md-12">
            <div class="form-group m-1">
                <div class="row">
                    <div class="col-6">
                        <h4>
                            <small><i class="fa fa-book mr-1"></i>สร้างนิติกรรมสัญญา > </small>
                            <small><span class="text-primary"><u>สร้างนิติกรรมสัญญาใหม่</u></span></small>
                        </h4>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="card mb-3"></div>
        </div>
    </div>
</section>



<div class="card accordion-group">
    <div class="card-header" id="headingDetailContract">
        <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailContract" aria-expanded="true" aria-controls="collapseDetailContract">
                <i class="fas fa-pencil-alt"></i> รายละเอียดนิติกรรมสัญญา
            </button>
        </h5>
    </div>
    <div id="collapseDetailContract" class="collapse show" aria-labelledby="headingDetailContract">
        <div class="card-body thead_tabsub1">
            <div class="form-group">
                <label class="lable_blue" for="">นิติกรรมสัญญา</label>
                <div class="list-group text-center">รายละเอียดข้อมูลนิติกรรมสัญญา</div>
                <hr class="my-4">
            </div>
        </div>
    </div>
</div>


<div class="card accordion-group">
    <div class="card-header" id="headingDetailVendor">
        <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailVendor" aria-expanded="true" aria-controls="collapseDetailVendor">
                <i class="fas fa-pencil-alt"></i> รายละเอียดคู่สัญญา
            </button>
        </h5>
    </div>
    <div id="collapseDetailVendor" class="collapse show" aria-labelledby="headingDetailVendor">
        <div class="card-body thead_tabsub1">
            <div class="form-group">
                <label class="lable_blue" for="">คู่สัญญา</label>
                <div class="list-group text-center">รายละเอียดข้อมูลคู่สัญญา</div>
                <hr class="my-4">
            </div>
        </div>
    </div>
</div>


<div class="card accordion-group">
    <div class="card-header" id="headingDetailContractPeriods">
        <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailContractPeriods" aria-expanded="true" aria-controls="collapseDetailContractPeriods">
                <i class="fas fa-pencil-alt"></i> รายละเอียดงวดงาน
            </button>
        </h5>
    </div>
    <div id="collapseDetailContractPeriods" class="collapse show" aria-labelledby="headingDetailContractPeriods">
        <div class="card-body thead_tabsub1">
            <div class="form-group">
                <label class="lable_blue" for="">งวดงาน</label>
                <div class="list-group text-center">รายละเอียดข้อมูลงวดงาน</div>
                <hr class="my-4">
            </div>
        </div>
    </div>
</div>


<div class="card accordion-group">
    <div class="card-header" id="headingDetailAttachFile">
        <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailAttachFile" aria-expanded="true" aria-controls="collapseDetailAttachFile">
                <i class="fas fa-file"></i> รายละเอียดไฟล์แนบท้ายสัญญา
            </button>
        </h5>
    </div>
    <div id="collapseDetailAttachFile" class="collapse show" aria-labelledby="headingDetailAttachFile">
        <div class="card-body thead_tabsub1">
            <div class="form-group">
                <label class="lable_blue" for="">ไฟล์แนบท้ายสัญญา</label>
                <div class="list-group text-center">รายละเอียดข้อมูลไฟล์แนบท้ายสัญญา</div>
                <hr class="my-4">
            </div>
        </div>
    </div>
</div>

<div class="card accordion-group">
        <div class="card-header" id="headingDetailContractFile">
            <h5 class="mb-0">
                <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailContractFile" aria-expanded="true" aria-controls="collapseDetailAttachFile">
                    <i class="fas fa-file-alt"></i> รายละเอียดแบบฟอร์มไฟล์สัญญา(ฉบับร่าง)
                </button>
            </h5>
        </div>
        <div id="collapseDetailContractFile" class="collapse show" aria-labelledby="headingDetailContractFile">
            <div class="card-body thead_tabsub1">
                <div class="form-group">
                    <label class="lable_blue" for="">แบบฟอร์มไฟล์สัญญา(ฉบับร่าง)</label>
                    <div class="list-group text-center">รายละเอียดแบบฟอร์มไฟล์สัญญา(ฉบับร่าง)</div>
                    <hr class="my-4">
                </div>
            </div>
        </div>
    </div>


<div class="card accordion-group">
    <div class="card-header" id="headingDetailVendorApprove">
        <h5 class="mb-0">
            <button class="btn btn-link" data-toggle="collapse" type="button" data-target="#collapseDetailVendorApprove" aria-expanded="true" aria-controls="collapseDetailVendorApprove">
                <i class="fas fa-file-alt"></i> รายละเอียดการลงนามหน่วยบริการ(ผู้มีอำนาจลงนาม)
            </button>
        </h5>
    </div>
    <div id="collapseDetailVendorApprove" class="collapse show" aria-labelledby="headingDetailVendorApprove">
        <div class="card-body thead_tabsub1">
            <div class="form-group">
                <label class="lable_blue" for="">การลงนามหน่วยบริการ(ผู้มีอำนาจลงนาม)</label>
                <div class="list-group text-center">รายละเอียดการลงนามหน่วยบริการ(ผู้มีอำนาจลงนาม)</div>
                <hr class="my-4">
            </div>
        </div>
    </div>
</div>

<br />
<div class="col-md-12">
    <div class="col text-center">
        <button type="button" class="btn btn-warning">บันทึกร่าง</button>&nbsp;
        @*<button type="button" class="btn btn-success">บันทึกส่งต่อ <i class="fa fa-angle-right align-middle"></i></button>*@
        <a class="btn btn-success" onclick="showLoading()" asp-area="" asp-controller="Home" asp-action="ContractApproveStation">บันทึกส่งต่อ</a>&nbsp;
        <button type="button" class="btn btn-dark"><i class="fa fa-angle-left align-middle"></i> ยกเลิกทำรายการ</button>
    </div>
</div>
<br />
<br />