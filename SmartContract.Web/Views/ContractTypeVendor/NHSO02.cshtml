                    </div>
                    <div class="col-sm-3 pr-sm-0">
                        <input type="text" class="form-control bg-focus" placeholder="ชื่อ">
                    </div>
                    <div class="col-sm-3 ">
                        <input type="text" class="form-control bg-focus" placeholder="นามสกุล">
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label text-sm-right">ตำแหน่ง :</label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control bg-focus" placeholder="">
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label text-sm-right">E-mail :</label>
                    <div class="col-sm-7">
                        <input type="email" class="form-control bg-focus" placeholder="example@nhso.go.th">
                    </div>
